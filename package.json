  "author": "Daniel Kovacs <daniel.kovacs@superbet.com>",
  "type": "module",
  "source": "./src/index.ts",
  "scripts": {
    "build": "vite build && tsc",
    "dev": "vite",
    "test": "vitest run"
  },
  "license": "MIT",
  "dependencies": {
    "path-to-regexp": "6.2.1"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^1.6",
    "react": "^16.8.3 || ^17",
    "react-redux": "^7.1.2",
    "react-router": "^5.0.0",
